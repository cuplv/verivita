# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.HomeController.index

# An example controller showing how to use dependency injection
GET     /count                      controllers.TraceController.count

GET     /trace                      controllers.TraceController.trace(traceId : String)

GET     /cxe                        controllers.TraceController.cxe(traceId: String, disallowId: String)

GET     /tracelist                  controllers.TraceController.getTraceList

+ nocsrf
POST    /parse_ls                   controllers.HomeController.parseLs

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

GET     /completion_search          controllers.HomeController.completionSearch

